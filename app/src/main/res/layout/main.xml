<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="center"
	android:orientation="vertical"
	android:id="@+id/mainLinearLayout3">
<!--妮哩萌萌！-->
	<LinearLayout
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		android:orientation="vertical"
		android:gravity="top|center"
		android:layout_weight="1.0"
		android:id="@+id/mainLinearLayout2">

		<LinearLayout
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_marginTop="18dp">

			<Button
				android:layout_height="wrap_content"
				android:text="振动1次"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton6"
				android:textSize="11sp"/>

			<Button
				android:layout_height="wrap_content"
				android:text="振动10次"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton7"
				android:textSize="11sp"/>

			<Button
				android:layout_height="wrap_content"
				android:text="振动n次"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton8"
				android:textSize="11sp"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_marginTop="18dp">

			<Button
				android:layout_height="wrap_content"
				android:text="开启报警音"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton1"
				android:textSize="11sp"/>

			<Button
				android:layout_height="wrap_content"
				android:text="关闭报警音"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton2"
				android:textSize="11sp"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_marginTop="20dp">

			<Button
				android:layout_height="wrap_content"
				android:text="开灯"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton3"
				android:textSize="11sp"/>

			<Button
				android:layout_height="wrap_content"
				android:text="关灯"
				android:layout_width="match_parent"
				android:layout_weight="1.0"
				android:id="@+id/mainButton4"
				android:textSize="11sp"/>

			<Button
				android:layout_height="wrap_content"
				android:layout_width="match_parent"
				android:id="@+id/mainButton5"
				android:onClick="ד"
				android:layout_weight="1.0"
				android:textSize="11sp"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_marginTop="20dp">

			<Button
				android:layout_height="wrap_content"
				android:layout_width="match_parent"
				android:text="选择本地视频"
				android:onClick="on1"
				android:layout_weight="1.0"
				android:textSize="11sp"/>

			<Button
				android:layout_height="wrap_content"
				android:layout_width="match_parent"
				android:onClick="on3"
				android:layout_weight="1.0"
				android:background="#00000000"
				android:visibility="gone"/>

			<Button
				android:layout_height="wrap_content"
				android:layout_width="match_parent"
				android:text="开始转换"
				android:onClick="on2"
				android:layout_weight="1.0"
				android:textSize="11sp"/>

		</LinearLayout>

		<TextView
			android:layout_height="wrap_content"
			android:text="妮哩萌萌"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:layout_width="wrap_content"
			android:layout_marginBottom="4dp"
			android:id="@+id/mainTextView2"/>

		<CheckBox
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:text="锁屏抢红包［耗电模式］"
			android:id="@+id/mainCheckBox1"
			android:textSize="12sp"/>

		<ScrollView
			android:layout_height="match_parent"
			android:layout_width="match_parent">

			<LinearLayout
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				android:orientation="vertical"
				android:gravity="bottom|center"
				android:id="@+id/mainLinearLayout1">

				<VideoView
					android:layout_height="wrap_content"
					android:id="@+id/videoView1"
					android:layout_width="wrap_content"/>

				<Button
					android:layout_height="wrap_content"
					android:text="已经默认支持QQ6.6.0-6.6.2抢红包［xposed］"
					android:layout_width="wrap_content"
					android:textSize="12sp"
					android:background="#00000000"/>

				<Button
					android:layout_height="wrap_content"
					android:text="网易云音乐vip说明［打开xposed&gt;模块.打勾|重启］"
					android:layout_width="wrap_content"
					android:textSize="12sp"
					android:background="#00000000"
					android:layout_margin="4dp"/>

				<TextView
					android:layout_height="wrap_content"
					android:text="ｸﾞｯ!(๑•̀ㅂ•́)و✧我的其他作品http://www.coolapk.com/apk/nico.styTool"
					android:layout_width="wrap_content"
					android:layout_marginBottom="8dp"
					android:textSize="12sp"
					android:autoLink="web"
					android:id="@+id/mainTextView1"/>

				<LinearLayout
					android:layout_height="wrap_content"
					android:layout_width="match_parent"
					android:orientation="vertical"
					android:gravity="center">

					<LinearLayout
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:orientation="horizontal">

						<EditText
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1.0"
							android:hint="？39.862559"
							android:layout_margin="4dp"
							android:id="@+id/mainEditText1"/>

						<EditText
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1.0"
							android:hint="？116.449535"
							android:layout_margin="4dp"
							android:id="@+id/mainEditText2"/>

					</LinearLayout>

					<TextView
						android:layout_height="wrap_content"
						android:text="经纬度"
						android:layout_width="wrap_content"/>

				</LinearLayout>

			</LinearLayout>

		</ScrollView>

	</LinearLayout>

</LinearLayout>

